{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "passw",
  "version": "0.1.0",
  "identifier": "com.jigonzalez930209.passw",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "",
        "width": 600,
        "height": 600,
        "resizable": false,
        "maximizable": false,
        "minWidth": 600,
        "minHeight": 600,
        "maxWidth": 600,
        "maxHeight": 600,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
